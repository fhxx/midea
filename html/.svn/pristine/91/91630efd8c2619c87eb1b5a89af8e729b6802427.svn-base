.box{
	display: -moz-box; display: -webkit-box; display: box; 
}
.flex{
	-moz-box-flex: 1;-webkit-box-flex: 1; box-flex: 1;
}
a:hover{
	text-decoration: none;
}
body{
   background: #efefef;
}
header{
	width: 100%; height: 50px; background: #000; position: fixed; top: 0px;z-index: 9;
}
header .user{
	cursor: pointer;
}
.c-fff{
	color: #fff;
}

 .radio-box{
 	padding-left: 0px; 
 }
.art-body .left{
	position: fixed; padding: 0px;
}
.art-body .right{
	padding: 0px;
}
.art-body .left ul,.art-body .left ul li{
	width: 100%;
}
.art-body .left ul li{
	line-height: 40px; line-height: 40px;
}
.art-body .left ul li a{
    color: #30a5ff; font-size: 1.25em; padding: 0 10px 0 15px; display: inline-block;
}
.art-body .left ul li a i{
	position: relative; right: 5px;
}
.art-body .left ul li:hover{
	background: #efefef;
}
.art-body .left ul li.active{
	background:#30a5ff ;
}
.art-body .left ul li.active a{
	color:#fff
}
.art-body .right nav{
	 height: 40px; line-height: 40px; background: #ddd; color: #999;
}

.art-body .right nav i{
	color: #30a5ff; font-size: 1.3em;
}
.select-body,.list-body{
	border:1px solid #ccc; background: #fff;
}
.select-i{
	width: 230px; height: 30px; margin: 15px 0px 0px 15px;
}
.select-i .l-box{
	width: 80px;line-height: 30px;
}
.select-i .r-box{
	width: 150px;
}
.select-box-input{
	width: 150px; 
}
.optionbtn{
	height: 40px; line-height: 40px;
}
.table{
	 width: 98%; margin-left: 1%;
}
.table th,.table-hover tbody tr:hover th{
	background: #CCC;
}
.table th,.table td{
	text-align: center;
}
.p-edit a{
	top:18px; right: 3px;
}
