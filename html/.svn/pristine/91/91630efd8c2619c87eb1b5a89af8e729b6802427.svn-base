.option-box{
	width: 120px; height: 0px; overflow: hidden;  box-shadow: 2px 2px 2px #999; background: #fff; right: 20px; top: 40px; border-radius: 5px;
}
.option-box ul li{
	width: 100%; cursor: pointer;
}
.option-box ul li:hover{
	background: #EEEEEE;
}
.option-box  li{
	height: 40px; float: left;
}
.option-box ul li .l-box{
	width: 40px; text-align: center; font-size: 20px;height: 40px; line-height: 40px;
}
.option-box ul li .r-box{
	width: 70px;height: 40px;  line-height: 40px; font-size: 1.2em;
}
